{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@adnbn-ui-config-app": [
        "./src/config/default.ts"
      ],
      "@adnbn-ui-config-shared": [
        "./src/config/default.ts"
      ],
      "@adnbn-ui-style-app": [
        "./src/styles/default.css"
      ],
      "@adnbn-ui-style-shared": [
        "./src/styles/default.css"
      ]
    }
  }
}
