{
  "name": "addon-ui",
  "type": "module",
  "version": "0.3.1",
  "description": "A comprehensive React UI component library designed exclusively for the AddonBone browser extension framework with customizable theming and consistent design patterns",
  "keywords": [
    "react",
    "ui",
    "components",
    "adnbn",
    "addonbone",
    "browser-extension",
    "theme",
    "customizable",
    "typescript",
    "design-system"
  ],
  "author": "Addon Stack",
  "contributors": [
    "Anjey Tsibylskij <addonbonedev@gmail.com> (https://github.com/atldays)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/addon-stack/addon-ui.git"
  },
  "bugs": {
    "url": "https://github.com/addon-stack/addon-ui/issues"
  },
  "types": "./src/index.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.ts"
      ],
      "config": [
        "./src/config/index.ts"
      ],
      "plugin": [
        "./src/plugin/index.ts"
      ]
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/config/index.ts",
    "./plugin": "./src/plugin/index.ts",
    "./theme": "./src/styles/mixins.scss"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "autosize": "^6.0.1",
    "classnames": "^2.5.1",
    "odometer": "^0.4.8",
    "radix-ui": "^1.1.3",
    "react-highlight-words": "^0.21.0",
    "sass": "^1.85.1",
    "ts-deepmerge": "^7.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@storybook/react": "^9.1.3",
    "@types/autosize": "^4.0.3",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-highlight-words": "^0.20.0",
    "adnbn": "^0.4.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rspack-plugin-virtual-module": "^1.0.0",
    "storybook": "^9.1.3",
    "storybook-react-rsbuild": "^2.1.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "adnbn": ">=0.2.9",
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
